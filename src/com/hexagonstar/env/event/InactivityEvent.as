/////////////////////////////////////////// 		// Properties                                                                         // 		//////////////////////////////////////////////////////////////////////////////////////// 				protected var _ms:uint; 		 		 		//////////////////////////////////////////////////////////////////////////////////////// 		// Public Methods                                                                     // 		//////////////////////////////////////////////////////////////////////////////////////// 				/**		 * Creates a new InactivityEvent.		 * 		 * @param type The type of event.		 * @param bubbles Determines whether the Event object participates in		 *         the bubbling stage of the event flow.		 * @param cancelable Determines whether the Event object can be canceled.		 */		public function InactivityEvent(type:String,											bubbles:Boolean = false,											cancelable:Boolean = false) 		{			super(type, bubbles, cancelable);		} 		 				/**		 * Clones the event object.		 * 		 * @return a clone of the event.		 */		override public function clone():Event		{			var e:InactivityEvent = new InactivityEvent(type, bubbles, cancelable);			e.milliseconds = milliseconds;			return e;		}				 		//////////////////////////////////////////////////////////////////////////////////////// 		// Getters & Setters                                                                  // 		//////////////////////////////////////////////////////////////////////////////////////// 				/**		 * The length of time an user has been inactive.		 */		public function get milliseconds():uint 		{			return _ms;		}				public function set milliseconds(v:uint):void 		{			_ms = v;		}	}}