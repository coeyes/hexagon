      		public function testAll():void		{			Debug.trace("\n\n*******************************************************\n"				+ "DLinkedListTest Tests"				+ "\n*******************************************************");						createEmpty();			create();						append();			add();			prepend();			insert();			replace();			removeByIndex();			removeFirst();			removeLast();			remove();						getElementAt();			contains();			indexOf();			join();			clone();			toArray();						addAll();			insertAll();			replaceAll();			containsAll();			removeAll();			retainAll();			clear();		}						private function log(title:String = ""):void		{			if (title.length > 0)			{				logTitle(title);			}			else			{				Debug.delimiter();			}						Debug.trace(_collection.dump());		}				private function logTitle(title:String = ""):void		{			Debug.trace("\n\n========================================\n"				+ "DLinkedListTest " + title				+ "\n========================================");		}						//--------------------------------------------------------------------------------------				private function createEmpty():void		{			_collection = new DLinkedList();			assertStrictlyEquals(_collection, _collection);			assertEquals(_collection.size, 0);			assertTrue(_collection.isEmpty);			log("create (empty)");		}				private function create():void		{			_collection = new DLinkedList("Jenny", "Brianna", "Keela");			assertStrictlyEquals(_collection, _collection);			assertEquals(_collection.size, 3);			assertFalse(_collection.isEmpty);			log("create");		}						//--------------------------------------------------------------------------------------				private function append():void		{			assertTrue(_collection.append("Heather"));			assertTrue(_collection.append(777));			assertTrue(_collection.append("Jeannie", "Cari", "Kayla"));			assertTrue(_collection.append(null));			assertFalse(_collection.append());			assertEquals(_collection.size, 9);			assertFalse(_collection.isEmpty);			log("append");		}				private function add():void		{			assertTrue(_collection.add("Suzanne"));			assertTrue(_collection.add(null));			assertEquals(_collection.size, 11);			assertFalse(_collection.isEmpty);			log("add");		}				private function prepend():void		{			assertTrue(_collection.prepend("Beth"));			assertTrue(_collection.prepend("Yoko", "Lucy", 125));			assertTrue(_collection.prepend(null));			assertFalse(_collection.prepend());			assertEquals(_collection.size, 16);			assertFalse(_collection.isEmpty);			log("prepend");		}				private function insert():void		{			assertTrue(_collection.insert(-12, "Inserted with Index -12"));			assertTrue(_collection.insert(0, "Inserted with Index 0"));			assertTrue(_collection.insert(1, "Inserted with Index 1"));			assertTrue(_collection.insert(3, "Inserted with Index 3"));			assertTrue(_collection.insert(7, "Inserted with Index 7"));			assertTrue(_collection.insert(15, "Inserted with Index 15"));			assertTrue(_collection.insert(21, "Inserted with Index 21"));			assertTrue(_collection.insert(999, "Inserted with Index 999"));			assertEquals(_collection.size, 24);			log("insert");		}				private function replace():void		{			assertEquals(_collection.replace(6, "Replaced with Arielle"), "Lucy");			assertEquals(_collection.replace(0, "Replaced with Donna"), "Inserted with Index 0");			assertEquals(_collection.replace(23, "Replaced with Thasa"), "Inserted with Index 999");			assertEquals(_collection.replace(22, "Replaced with Conny"), null);			assertEquals(_collection.size, 24);			log("replace");		}				private function removeByIndex():void		{			assertEquals(_collection.removeByIndex(4), null);			assertEquals(_collection.removeByIndex(16), "Cari");			assertEquals(_collection.size, 22);			log("removeByIndex");		}				private function removeFirst():void		{			assertEquals(_collection.removeFirst(), "Replaced with Donna");			assertEquals(_collection.removeFirst(), "Inserted with Index 1");			assertEquals(_collection.size, 20);			log("removeFirst");		}				private function removeLast():void		{			assertEquals(_collection.removeLast(), "Replaced with Thasa");			assertEquals(_collection.removeLast(), "Replaced with Conny");			assertEquals(_collection.size, 18);			log("removeLast");		}				private function remove():void		{			assertEquals(_collection.remove("Inserted with Index -12"), "Inserted with Index -12");			assertEquals(_collection.remove("Brianna"), "Brianna");			assertEquals(_collection.remove("Inserted with Index 21"), "Inserted with Index 21");			assertEquals(_collection.remove(777), 777);			assertEquals(_collection.remove(null), null);			assertUndefined(_collection.remove("XXXX"));						assertTrue(_collection.insert(3, "BethNew"));			log("remove");						assertEquals(_collection.remove("Beth"), "Beth");			assertEquals(_collection.size, 13);			log();		}						//--------------------------------------------------------------------------------------				private function getElementAt():void		{			assertEquals(_collection.getElementAt(0), "Inserted with Index 3");			assertEquals(_collection.getElementAt(1), "Yoko");			assertEquals(_collection.getElementAt(5), 125);			assertEquals(_collection.getElementAt(10), "Jeannie");			assertEquals(_collection.getElementAt(12), "Suzanne");			//assertEquals(_collection.size, 13);			log("getElementAt");		}				private function contains():void		{			assertTrue(_collection.contains("Inserted with Index 3"));			assertFalse(_collection.contains("Inserted with Index 7000"));			assertTrue(_collection.contains(125));			assertFalse(_collection.contains(126));			assertTrue(_collection.contains("Suzanne"));			assertFalse(_collection.contains(null));			assertFalse(_collection.contains(undefined));			assertEquals(_collection.size, 13);			log("contains");		}				private function indexOf():void		{			assertEquals(_collection.indexOf("Inserted with Index 3"), 0);			assertEquals(_collection.indexOf("Yoko"), 1);			assertEquals(_collection.indexOf(125), 5);			assertEquals(_collection.indexOf("Kayla"), 11);			assertEquals(_collection.indexOf("Suzanne"), 12);			assertEquals(_collection.indexOf("Susan"), -1);			assertEquals(_collection.size, 13);			log("indexOf");		}				private function join():void		{			Debug.trace("\n\n" + _collection.join());		}				private function clone():void		{			var clone:DLinkedList = _collection.clone();			logTitle("clone");			Debug.trace("Clone:\n" + clone.dump());			assertEquals(clone.size, 13);		}				private function toArray():void		{			var array:Array = _collection.toArray();			logTitle("toArray");			Debug.trace(array);			assertEquals(array.length, 13);		}						//--------------------------------------------------------------------------------------			private function addAll():void		{			var list2:DLinkedList = new DLinkedList("Steve", "Gerry", "James");			assertTrue(_collection.addAll(list2));						var emptyList:DLinkedList = new DLinkedList();			assertFalse(_collection.addAll(emptyList));						assertEquals(_collection.size, 16);			log("addAll");		}				private function insertAll():void		{			var list3:DLinkedList = new DLinkedList("Buffy", "Giles", "Spike");			assertTrue(_collection.insertAll(4, list3));			var emptyList:DLinkedList = new DLinkedList();						assertFalse(_collection.insertAll(0, emptyList));			assertEquals(_collection.size, 19);			log();						var list4:DLinkedList = new DLinkedList("Dawn", "Willow");			assertTrue(_collection.insertAll(19, list4));						assertEquals(_collection.size, 21);			log("insertAll");		}				private function replaceAll():void		{			var list5:DLinkedList = new DLinkedList("Replaced with Magnum", "Replaced with Higgins", "Replaced with TC");			assertTrue(_collection.replaceAll(1, list5));						var emptyList:DLinkedList = new DLinkedList();			assertFalse(_collection.replaceAll(12, emptyList));						assertEquals(_collection.size, 21);			log("replaceAll");		}				private function containsAll():void		{			var list6:DLinkedList = new DLinkedList("Giles", "Willow", 125, "Keela");			assertTrue(_collection.containsAll(list6));						var list7:DLinkedList = new DLinkedList("Giles", "Willow", 125, "Raoul");			assertFalse(_collection.containsAll(list7));						assertEquals(_collection.size, 21);			log("containsAll");		}				private function removeAll():void		{			var list8:DLinkedList = new DLinkedList("Buffy", "Steve", "Jeannie");			assertTrue(_collection.removeAll(list8));						var list9:DLinkedList = new DLinkedList("James", "Todd", "Terry");			assertTrue(_collection.removeAll(list9));						var list10:DLinkedList = new DLinkedList("Cora", "Todd", "Terry");			assertFalse(_collection.removeAll(list10));						assertEquals(_collection.size, 17);			log("removeAll");		}				private function retainAll():void		{			var list11:DLinkedList = new DLinkedList();			list11.append("Giles", "Keela", "Dawn", 125, "Kayla", "Anabel");			assertTrue(_collection.retainAll(list11));						var list12:DLinkedList = new DLinkedList();			list12.append("Giles", "Keela", "Dawn", 125, "Kayla", "Anabel");			assertFalse(_collection.retainAll(list12));			assertEquals(_collection.size, 5);						var list13:DLinkedList = new DLinkedList("Anabel", "Suzi", "Bill");			assertTrue(_collection.retainAll(list13));			assertEquals(_collection.size, 0);			assertTrue(_collection.isEmpty);						log("retainAll");		}				private function clear():void		{			_collection.append("Anabel", "Suzi", "Bill");			assertEquals(_collection.size, 3);			assertFalse(_collection.isEmpty);			log("clear");						_collection.clear();			assertEquals(_collection.size, 0);			assertTrue(_collection.isEmpty);			log();		}	}}