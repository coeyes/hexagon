////////////////////////		// Getters & Setters                                                                  //		////////////////////////////////////////////////////////////////////////////////////////				public function get isCollectable():Boolean		{			return _isCollectable;		}				public function set isCollectable(v:Boolean):void		{			_isCollectable = v;		}				public function get isMoveable():Boolean		{			return _isMoveable;		}				public function set isMoveable(v:Boolean):void		{			_isMoveable = v;		}				public function get isContainer():Boolean		{			return _isContainer;		}				public function set isContainer(v:Boolean):void		{			_isContainer = v;		}	}}