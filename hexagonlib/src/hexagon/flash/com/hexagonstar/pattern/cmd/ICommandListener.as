package com.hexagonstar.pattern.cmd{	import com.hexagonstar.event.CommandEvent;		/**	 * Classes that want to informed by events broadcasted from Commands should	 * implement this interface.	 * 	 * @author Sascha Balkau	 */	public interface ICommandListener	{		function onCommandProgress(e:CommandEvent):void;		function onCommandComplete(e:CommandEvent):void;		function onCommandAbort(e:CommandEvent):void;		function onCommandError(e:CommandEvent):void;	}}