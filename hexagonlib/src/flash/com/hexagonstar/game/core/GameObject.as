	/**	 * GameObject class	 */	public class GameObject extends BasicClass	{		////////////////////////////////////////////////////////////////////////////////////////		// Variables                                                                          //		////////////////////////////////////////////////////////////////////////////////////////				protected var _id:String;		protected var _name:String = "";						////////////////////////////////////////////////////////////////////////////////////////		// Public Methods                                                                     //		////////////////////////////////////////////////////////////////////////////////////////				/**		 * Creates a new GameObject instance.		 */		public function GameObject()		{		}						/**		 * Returns a String Representation of GameObject.		 * 		 * @return A String Representation of GameObject.		 */		override public function toString(...args):String		{			return "[GameObject, id=" + _id + "]";		}						////////////////////////////////////////////////////////////////////////////////////////		// Getters & Setters                                                                  //		////////////////////////////////////////////////////////////////////////////////////////				public function get id():String		{			return _id;		}				public function set id(v:String):void		{			_id = v;		}				public function get name():String		{			return _name;		}				public function set name(v:String):void		{			_name = v;		}	}}