package com.hexagonstar.framework.command.file{	import com.hexagonstar.framework.io.loaders.ConfigLoader;		/**	 * LoadConfigCommand Class	 * @author Sascha Balkau	 */	public class LoadConfigCommand extends AbstractLoadCommand	{		////////////////////////////////////////////////////////////////////////////////////////		// Public Methods                                                                     //		////////////////////////////////////////////////////////////////////////////////////////				/**		 * Creates a new LoadConfigCommand instance.		 */		public function LoadConfigCommand()		{			super();		}						/**		 * Execute the command.		 */ 		override public function execute():void		{			/* Initialize the config model before we load any config data */			Main.config.init();						_loader = new ConfigLoader("app.ini", "appIniFile");						super.execute();		}						////////////////////////////////////////////////////////////////////////////////////////		// Getters & Setters                                                                  //		////////////////////////////////////////////////////////////////////////////////////////				override public function get name():String		{			return "loadConfig";		}	}}